    <body>
      <trans-unit id="title" resname="title">
        <source>Request a Quote</source>
        <target>Solicitar Cotización</target>
      </trans-unit>
      <trans-unit id="description" resname="description">
        <source>Allow customers to request quotes instead of direct purchases. Professional B2B solution.</source>
        <target>Permite a los clientes solicitar cotizaciones en lugar de compras directas. Solución B2B profesional.</target>
      </trans-unit>
      <trans-unit id="button_text" resname="button_text">
        <source>Request a Quote</source>
        <target>Solicitar Cotización</target>
      </trans-unit>
      <trans-unit id="hide_price" resname="hide_price">
        <source>Hide Price on Quote Products</source>
        <target>Ocultar Precio en Productos de Cotización</target>
      </trans-unit>
      <trans-unit id="hide_buy_button" resname="hide_buy_button">
        <source>Hide Buy Button (Replace with Quote)</source>
        <target>Ocultar Botón de Compra (Reemplazar con Cotización)</target>
      </trans-unit>
      <trans-unit id="enable_quote" resname="enable_quote">
        <source>Enable Quote Button</source>
        <target>Habilitar Botón de Cotización</target>
      </trans-unit>
      <trans-unit id="quote_button_text" resname="quote_button_text">
        <source>Quote Button Text</source>
        <target>Texto del Botón de Cotización</target>
      </trans-unit>
      <trans-unit id="message_optional" resname="message_optional">
        <source>Message (optional)</source>
        <target>Mensaje (opcional)</target>
      </trans-unit>
      <trans-unit id="placeholder" resname="placeholder">
        <source>Any special request or question?</source>
        <target>¿Alguna solicitud especial o pregunta?</target>
      </trans-unit>
      <trans-unit id="success_message" resname="success_message">
        <source>Quote request sent successfully!</source>
        <target>¡Solicitud de cotización enviada exitosamente!</target>
      </trans-unit>
      <trans-unit id="error_message" resname="error_message">
        <source>An error occurred. Please try again.</source>
        <target>Ocurrió un error. Por favor, intente de nuevo.</target>
      </trans-unit>
      <trans-unit id="must_login" resname="must_login">
        <source>You must be logged in to request a quote</source>
        <target>Debe iniciar sesión para solicitar una cotización</target>
      </trans-unit>
      <trans-unit id="new_quote" resname="new_quote">
        <source>New Quote Request</source>
        <target>Nueva Solicitud de Cotización</target>
      </trans-unit>
    </body>
